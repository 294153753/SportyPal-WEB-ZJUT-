<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="po.content_statistics" table="content_statistics" catalog="db_sportsys">
		<id name="id" type="int">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="forward" type="int">
			<column name="forward" />
		</property>
		<property name="comment" type="int">
			<column name="comment" />
		</property>
		<property name="likes" type="int">
			<column name="likes" />
		</property>
	</class>
</hibernate-mapping>
